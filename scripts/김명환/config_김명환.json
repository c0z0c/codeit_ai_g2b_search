{
  "version": "1.0.0",
  "OPENAI_MODEL": "gpt-4o-mini",
  "OPENAI_TEMPERATURE": 0.0,
  "OPENAI_EMBEDDING_MODEL": "text-embedding-3-small",
  "OPENAI_TOKENIZER_MODEL": "gpt-4",
  "CHUNKING_MODE": "token",
  "CHUNK_SIZE": 600,
  "CHUNK_OVERLAP": 100,
  "CHUNK_SEPARATORS": [
    "\n\n",
    "\n",
    ". ",
    "! ",
    "? ",
    ", ",
    " ",
    ""
  ],
  "SUMMARY_RATIO": 0.2,
  "SUMMARY_OVERLAP_RATIO": 0.1,
  "SUMMARY_MIN_LENGTH": 100,
  "SIMILARITY_THRESHOLD": 0.75,
  "TOP_K_SUMMARY": 5,
  "TOP_K_FINAL": 2,
  "SCORE_GAP_THRESHOLD": 0.15,
  "EMBEDDING_BATCH_SIZE": 100,
  "EMBEDDING_DIMENSION": 1536,
  "DATA_PATH": "data",
  "DOCUMENTS_DB_PATH": "d:\\GoogleDrive\\codeit_ai_g2b_search\\data\\documents.db",
  "EMBEDDINGS_DB_PATH": "d:\\GoogleDrive\\codeit_ai_g2b_search\\data\\embeddings.db",
  "CHAT_HISTORY_DB_PATH": "d:\\GoogleDrive\\codeit_ai_g2b_search\\data\\chat_history.db",
  "VECTORSTORE_PATH": "d:\\GoogleDrive\\codeit_ai_g2b_search\\data\\vectorstore",
  "CONFIG_PATH": "config_김명환.json",
  "EMPTY_PAGE_THRESHOLD": 10,
  "ERROR_PAGE_MARKER": "--- [오류페이지] ---",
  "EMPTY_PAGE_MARKER": "--- [빈페이지] ---",
  "PAGE_MARKER_FORMAT": "--- 페이지 {page_num} ---",
  "TOKEN_ESTIMATION_DIVISOR": 4,
  "MARKDOWN_PROTECT_BLOCKS": [
    "code",
    "math",
    "inline_math",
    "mermaid"
  ],
  "MARKDOWN_REMOVE_ELEMENTS": [
    "html",
    "images",
    "links",
    "emphasis",
    "headers",
    "blockquotes"
  ],
  "MARKDOWN_MAX_LINES": {
    "code": 100,
    "math": 50
  },
  "HASH_ALGORITHM": "sha256",
  "LOG_LEVEL": "DEBUG",
  "LOG_DIR": "logs",
  "LOG_FILE_NAME": "rag_system.log",
  "LOG_FILE_MAX_BYTES": 10485760,
  "LOG_FILE_BACKUP_COUNT": 5,
  "RAG_PROMPT_TEMPLATE": "다음 문서를 참고하여 질문에 답변해주세요.\n\n참고 문서:\n{context}\n\n질문: {query}\n\n답변:",
  "NO_CONTEXT_MESSAGE": "관련 문서를 찾을 수 없습니다.",
  "CONTEXT_FORMAT": "[문서 {index}: {file_name}]\n{chunk_text}"
}